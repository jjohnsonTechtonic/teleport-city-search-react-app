   getImageByCity(cityDetails.name).then(res => {\n        if (res.ok){\n          return res.json();\n        }\n      }).then(data => {\n        if (typeof data === 'object'){\n          setImageUrl(data.photos[0].image.web);\n        }\n        else{\n          setImageUrl('');\n        }\n      })\n      .catch(err => console.log(err));\n    }\n  },[cityDetails]);\n  return<> \n  <h1>Teleport City Search</h1>\n  <main className=\"wrapper\">\n  <SearchContext.Provider value={{...searchData, setTerm, setResults}}>\n    <SearchBar/>\n    <SearchResults setCityDetails={setCityDetails}/>\n  </SearchContext.Provider>\n  {cityDetails?<CityDetails details={cityDetails} imageUrl={imageUrl}/>:<></>}\n  </main></>;\n};\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"497bf271fd1c300c67aa10e216e8aed8","cacheData":{"env":{}}}