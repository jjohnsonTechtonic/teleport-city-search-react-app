rn <div className=\"details__scores\">\n        {scoreDetails.summary?\n            <>\n                <h4>Overall Score: {Math.round(scoreDetails.overall)}/100</h4>\n                <p dangerouslySetInnerHTML={{__html: scoreDetails.summary}}></p>\n                <ul>\n                    {scoreDetails.categories.map(({name,score_out_of_10}) => {\n                        return <li key={`${name}-${score_out_of_10}`}>{name}: <span>{score_out_of_10.toFixed(1)}</span>/10</li>\n                    })}\n                </ul>\n            </>\n        :<p>No details available for this location</p>}\n    </div>\n};\n\nexport default CityScores;"},"lineCount":null}},"error":null,"hash":"b40c161e02b5b0629ba2dfb2d8c8bf88","cacheData":{"env":{}}}