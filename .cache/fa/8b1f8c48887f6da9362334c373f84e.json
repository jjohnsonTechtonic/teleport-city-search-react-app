ows: 40px auto;\n  grid-template-areas: 'searchbar details' 'search-results details';\n  padding: 1rem;\n}\n\n.search{\n  &__bar{\n    grid-area: searchbar;\n    display: flex;\n    width: 100%;\n    input{\n      padding-left: 0.5rem;\n      width: 100%;\n    }\n  }\n  &__results{\n    grid-area: search-results;\n    height: 100vh;\n    overflow-y: scroll;\n    width: 100%;\n  }\n}\n\n.details{\n  grid-area: details;\n  padding: 1rem;\n  height: 100vh;\n  text-align: center;\n  // overflow-y: scroll;\n  img{\n    width: 100%;\n  }\n  &__scores{\n    ul{\n      list-style-type: none;\n      text-align: left;\n      padding-top: 1rem;\n      li{\n        display: inline-block;\n        width: 50%;\n        span{\n          color: $carrot-orange;\n          font-size: larger;\n        }\n      }\n    }\n  }\n}\n\n.btn{\n  padding: 1rem;\n  width: 100%;\n  border-radius: 0;\n  border-style: none;\n  background-color: $blue-ncs;\n  color: $gainsboro;\n  &:hover{\n    background-color: $carrot-orange;\n    color: $raisin-black;\n  }\n  &--search{\n    width: auto;\n    display: flex;\n    align-items: center;\n    * {\n      padding: 0 0.2rem;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"2fca9492b5eb8d285507b0710b79aa05","cacheData":{"env":{}}}